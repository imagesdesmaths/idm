<types>
	<type>
		<field>code_postal</field>
		<label><multi>[fr]Code Postal[en]Postal Code[nl]Postcode</multi></label>
		<match>/^[0-9]{4,5}$/</match>
	</type>
	<type>
		<field>heure</field>
		<label><multi>[fr]Heure[en]Hour[nl]Huur</multi></label>
		<match>/^\s*[1-2]?[0-9][:hH][0-5]?[0-9]\s*$/</match>
	</type>
	<type>
		<field>telephone</field>
		<label><multi>[fr]T&eacute;l&eacute;phone[en]Phone Number[nl]Telefoonnummer</multi></label>
		<match>/^(((([\+])|(00[.-\s]))?([1-9]){2}[.-\s]?[1-689][.-\s]?)|(0[.-\s]?[1-689][.-\s]?))([0-9]{2}[.-\s]?){4}$/</match>
		<formate>
			<match>/[\s]+/</match>
			<replace>.</replace>
		</formate>
	</type>
</types>